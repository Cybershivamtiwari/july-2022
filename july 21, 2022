public class Solution {
    public ListNode reverseBetween(ListNode head, int m, int n) {
        ListNode dummy = new ListNode(-1);
        dummy.next = head;
        ListNode p = dummy;
        int k = 0;
        while( ++k < m){
            if(p != null) p = p.next;
        }
        //TODO:
        ListNode tail = p.next;
        while(++k <= n){
            ListNode tmp = p.next;

            p.next = tail.next;
            tail.next = tail.next.next;
            p.next.next = tmp;


        }

        return dummy.next;


    }
}
